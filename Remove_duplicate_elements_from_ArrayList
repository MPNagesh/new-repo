public class Program {
	public static void main(String[] args) {
	// Program 16 - remove duplicate elements from ArrayList
		ArrayList al = new ArrayList();
		al.add(20);
		al.add(50);
		al.add(30);
		al.add(60);
		al.add(20);
		al.add(10);
		al.add(60);
		System.out.println("ArrayList Elements : "+ al);
        HashSet set = new HashSet(al);
        ArrayList al1 = new ArrayList(set);
        System.out.print("ArrayList After Removing Duplicate Elements : "+al1);
	    }
	}
